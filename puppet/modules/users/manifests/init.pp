class users {
  user { 'deployer':
    ensure     => present,
    managehome => true,
    groups     => 'wheel',
  }

  ssh_authorized_key { 'deployer_key':
    ensure  => present,
    key     => '',
    type    => 'rsa',
    user    => 'deployer',
    require => User['deployer'],
  }

  users::irc {
    'nfisher': key => '';

    'nferrier': key => '', type => 'ssh-dss';

    'ppotter': key  => '';

    'ihassin': key  => '';

    'mryall': key => '';

    'jsmith': key => '';

    'jgray': key => '',

    'sdqali': key => ''
  }
}
